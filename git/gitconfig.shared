[core]
    excludesfile = ~/.gitignore_global
    autocrlf = input
    editor = "code --wait"
[init]
    defaultBranch = "master"
[commit]
    template = ~/.gitmessage
[user]
    useConfigOnly = true
[pull]
    rebase = false
[push]
    default = simple
[rerere]
    enabled = true
[color]
    ui = true
[pager]
    branch = false
